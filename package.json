    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^9.0.8",
