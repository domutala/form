{
  "name": "bank.webapp",
  "private": true,
  "type": "module",
  "version": "beta-0.7.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nuxt generate",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start:prod": "node ./serve.js",
    "ionic:build": "pnpm ionic:gen-icon && pnpm generate",
    "ionic:serve": "pnpm dev",
    "ionic:dev": "yarn ionic:gen-icon && ionic cap run --external -livereload --consolelogs",
    "ionic:gen-icon": "npx @capacitor/assets generate --iconBackgroundColor #ffffff --iconBackgroundColorDark #000000 --splashBackgroundColor #ffffff --splashBackgroundColorDark #0e0e0e --logoSplashScale 0.1 --assetPath assets/resources"
  },
